            <TextBlock Height="28.54" TextWrapping="Wrap" Text="グラフ" Width="86.25" FontSize="20"/>
        </ListBox>
        <ListBox HorizontalAlignment="Left" Height="140.25" Margin="0,299.75,0,0" VerticalAlignment="Top" Width="800">
            <TextBlock Height="28.54" TextWrapping="Wrap" Text="カード合計/部署名" Width="156.25" FontSize="20"/>
        </ListBox>
        <Button Content="戻る" Command="{Binding Home2Command}" HorizontalAlignment="Left" Height="31.875" VerticalAlignment="Top" Width="48.5" Background="#FFDDDDDD" BorderBrush="Black" FontSize="20"/>
    </Grid>
</UserControl>
