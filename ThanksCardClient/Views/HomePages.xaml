<UserControl x:Class="ThanksCardClient.Views.HomePages"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ThanksCardClient.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/Views/Thank Card Done (13).png"/>
        </Grid.Background>
        <Button Content="ホームボタン" Command="{Binding HomeCommand}" HorizontalAlignment="Center" Margin="0,362,0,0" VerticalAlignment="Top" Height="35" Width="92" BorderBrush="{x:Null}" Foreground="{x:Null}">
            <Button.Background>
                <ImageBrush ImageSource="/Views/Mask group.png"/>
            </Button.Background>
        </Button>


    </Grid>
</UserControl>
